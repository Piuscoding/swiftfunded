        align-items: center;
        color: white;
        border: 1px solid dodgerblue;
        border-radius: 3px;
        margin: 5px 0;
    }
    form sup {
        color: red;
    }
    .tab {
        display: flex; 
        align-items: center; 
        padding: 5px 10px;
        border-radius: 3px;
        border: 2px solid var(--text);
        margin: 5px 0;
    }
    .active-tab {
        border: 2px solid dodgerblue;
        box-shadow: 1px 1px 10px dodgerblue;
    }
    .fa-check-square {
        color: dodgerblue;
    }
    #label-file {
                        position: relative;
                        width: 100%;
                        height: 200px;
                        border-radius: 25px;
                        background: transparent;
                        box-shadow: 0 4px 7px rgba(0, 0, 0, 0.4);
                        display: flex;
                        align-items: center;
                        justify-content: center;
                        color: #fff;
                        font-weight: bold;
                        cursor: pointer;
                        transition: transform .2s ease-out; 
                        overflow: hidden;               
                       }
                    #file {
                        opacity: 0;
                        width: 100%;
                        height: 100%;
                        position: absolute;
                    }

